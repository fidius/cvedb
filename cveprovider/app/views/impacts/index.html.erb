<h1>Listing impacts</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @impacts.each do |impact| %>
  <tr>
    <td><%= impact.name %></td>
    <td><%= link_to 'Show', impact %></td>
    <td><%= link_to 'Edit', edit_impact_path(impact) %></td>
    <td><%= link_to 'Destroy', impact, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Impact', new_impact_path %>
